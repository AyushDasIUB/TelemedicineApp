      <Button layoutX="409.0" layoutY="376.0" mnemonicParsing="false" onAction="#patientHistoryOnAction" prefHeight="36.0" prefWidth="153.0" style="-fx-background-color: #3F2B63; -fx-background-radius: 20px 20px 20px 20px; -fx-effect: dropshadow(gaussian, rgba(0, 0, 10, 0.3), 10, 0, 10, 10);" text="Patient History" textFill="WHITE">
         <font>
            <Font name="Bell MT" size="14.0" />
         </font>
      </Button>
      <Button layoutX="39.0" layoutY="376.0" mnemonicParsing="false" onAction="#writeReportOnAction" prefHeight="36.0" prefWidth="153.0" style="-fx-background-color: #3F2B63; -fx-background-radius: 20px 20px 20px 20px; -fx-effect: dropshadow(gaussian, rgba(0, 0, 10, 0.3), 10, 0, 10, 10);" text="Write Report" textFill="WHITE">
         <font>
            <Font name="Bell MT" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
