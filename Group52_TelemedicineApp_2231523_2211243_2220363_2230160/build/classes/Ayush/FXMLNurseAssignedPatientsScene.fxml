          <TableColumn fx:id="nameTableColumn" prefWidth="108.0" text="Name" />
          <TableColumn fx:id="idTableColumn" prefWidth="86.0" text="ID" />
            <TableColumn fx:id="genderTableColumn" prefWidth="59.0" text="Gender" />
            <TableColumn fx:id="dobTableColumn" prefWidth="91.0" text="Date Of Birth" />
