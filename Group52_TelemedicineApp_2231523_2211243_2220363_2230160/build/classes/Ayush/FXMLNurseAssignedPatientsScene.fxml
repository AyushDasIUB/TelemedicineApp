        </columns>
      </TableView>
      <Button layoutX="220.0" layoutY="376.0" mnemonicParsing="false" onAction="#contactPatientOnAction" prefHeight="36.0" prefWidth="153.0" style="-fx-background-color: #3F2B63; -fx-background-radius: 20px 20px 20px 20px; -fx-effect: dropshadow(gaussian, rgba(0, 0, 10, 0.3), 10, 0, 10, 10);" text="Contact Patient" textFill="WHITE">
